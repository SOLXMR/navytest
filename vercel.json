{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/static/$1"
    },
    {
      "src": "/logo1.png",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/logo1.png"
    },
    {
      "src": "/manifest.json",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" },
      "dest": "/manifest.json"
    },
    {
      "src": "/(.+)\\.(?:ico|png|jpg|jpeg|gif|webp|svg)$",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" },
      "dest": "/index.html"
    }
  ]
} 